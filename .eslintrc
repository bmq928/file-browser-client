{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": false,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [1, 80, 2, { "ignoreComments": true }],
    "no-unused-vars": "off",
    "no-console": "off",
    "indent": ["2"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error"],
    "default-case": "off"
  }
}